<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.MenuAdmController">
    <stylesheets>
        <URL value="@/style.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="202.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/logo.png" />
            </image></ImageView>
        <Label fx:id="alchimicus_logo" layoutX="202.0" layoutY="178.0" text="Alchimicus" textFill="WHITE">
            <font>
                <Font name="Yu Gothic Bold" size="37.0" />
            </font></Label>
        <Label fx:id="administratione" layoutX="226.0" layoutY="223.0" text="Menu de Administração" textFill="WHITE">
            <font>
                <Font name="Yu Gothic Bold" size="13.0" />
            </font></Label>
        <HBox alignment="CENTER" layoutX="1.0" layoutY="257.0" prefHeight="100.0" prefWidth="600.0" spacing="10.0">
            <children>
                <Button fx:id="btn_Estoque" layoutX="169.0" layoutY="604.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnEstoque_Click" text="Estoque">
                    <font>
                        <Font name="Yu Gothic Bold" size="18.0" />
                    </font></Button>
                <Button fx:id="btn_Clientes" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnClientes_Click" text="Clientes">
                    <font>
                        <Font name="Yu Gothic Bold" size="18.0" />
                    </font></Button>
                <Button fx:id="btn_HistoricoVendas" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnHistoricoVendas_Click" text="Historico de Vendas">
                    <font>
                        <Font name="Yu Gothic Bold" size="16.0" />
                    </font>
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></HBox>
        <Button fx:id="btn_logout" layoutX="14.0" layoutY="14.0" minHeight="16.0" minWidth="19.0" mnemonicParsing="false" onAction="#btnLogout_Click" prefHeight="35.0" prefWidth="60.0" text="Voltar">
            <font>
                <Font name="Yu Gothic Bold" size="13.0" />
            </font></Button>
    </children>
</AnchorPane>
