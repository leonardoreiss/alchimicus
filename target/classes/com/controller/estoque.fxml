<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.EstoqueController">
    <stylesheets>
        <URL value="@/style.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="202.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/logo.png" />
            </image></ImageView>
        <Label fx:id="alchimicus_logo" layoutX="202.0" layoutY="178.0" text="Alchimicus" textFill="WHITE">
            <font>
                <Font name="Yu Gothic Bold" size="37.0" />
            </font></Label>
        <Label fx:id="administratione" layoutX="271.0" layoutY="223.0" text="Estoque" textFill="WHITE">
            <font>
                <Font name="Yu Gothic Bold" size="13.0" />
            </font></Label>
        <HBox alignment="CENTER" layoutX="1.0" layoutY="271.0" prefHeight="100.0" prefWidth="600.0" spacing="10.0">
            <children>
                <Button fx:id="btn_adicionarItens" layoutX="169.0" layoutY="604.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnadicionarItens_Click" text="Novo Item">
                    <font>
                        <Font name="Yu Gothic Bold" size="16.0" />
                    </font></Button>
            <Button fx:id="btn_verEstoque" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnVerEstoque_Click" text="Ver Estoque">
               <font>
                  <Font name="Yu Gothic Bold" size="16.0" />
               </font>
            </Button>
                <Button fx:id="btn_novaCategoria" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnNovoCategoria_Click" text="Nova Categoria">
                    <font>
                        <Font name="Yu Gothic Bold" size="16.0" />
                    </font></Button>
                <Button fx:id="btn_Fornecedor" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnFornecedor_Click" text="Novo Fornecedor">
                    <font>
                        <Font name="Yu Gothic Bold" size="16.0" />
                    </font></Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></HBox>
        <Button fx:id="btn_back" layoutX="14.0" layoutY="14.0" minHeight="16.0" minWidth="19.0" mnemonicParsing="false" onAction="#btnBack_Click" prefHeight="27.0" prefWidth="53.0" text="Voltar">
            <font>
                <Font name="Yu Gothic Bold" size="11.0" />
            </font></Button>
    </children>
</AnchorPane>
